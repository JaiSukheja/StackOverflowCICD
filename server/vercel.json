{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "MONGO_URI": ""
    },
    "functions": {
      "index.js": {
        "memory": 1024,
        "maxDuration": 60
      }
    }
  }